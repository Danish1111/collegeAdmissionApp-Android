<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/header_view"
        style="@style/MatchWidth"
        android:background="@color/status_red"
        android:elevation="6dp"
        android:orientation="horizontal">

        <com.example.abc.itmcollegealigarh.view.MaterialRippleLayout
            style="@style/WrapContent"
            app:mrl_rippleOverlay="true">

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/iv_navigation"
                android:tint="@color/black"
                style="@style/WrapContent"
                app:tint="@color/white"
                android:layout_gravity="center_vertical"
                app:srcCompat="@drawable/ic_left_arrow"
                android:padding="15dp" />
        </com.example.abc.itmcollegealigarh.view.MaterialRippleLayout>

        <LinearLayout
            style="@style/WrapContent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <com.example.abc.itmcollegealigarh.view.TextViewPlus
                android:id="@+id/tv_header_title"
                style="@style/WrapContent"
                android:padding="15dp"
                android:text="Infrastructure"
                android:textColor="@color/white"
                android:textSize="18sp"
                app:customFont="@string/GothamMedium" />

        </LinearLayout>

    </RelativeLayout>

    <android.support.v4.widget.NestedScrollView style="@style/MatchWidth">

        <LinearLayout
            style="@style/MatchWidth"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="15dp"
                android:orientation="horizontal">

                <com.example.abc.itmcollegealigarh.view.MaterialRippleLayout
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:id="@+id/ll_lecture_theater"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="2dp"
                        android:background="@drawable/counter_rectangle_shape"
                        android:orientation="vertical">
                        <ImageView
                            android:layout_width="match_parent"
                            android:background="@drawable/lecturetheater"
                            android:layout_height="120dp" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Lecture Theatre"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:fontFamily="sans-serif-condensed"
                            android:textSize="16sp"
                            android:gravity="center"/>

                    </LinearLayout>

                </com.example.abc.itmcollegealigarh.view.MaterialRippleLayout>


                <com.example.abc.itmcollegealigarh.view.MaterialRippleLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1">

                    <LinearLayout
                        android:id="@+id/ll_library"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="2dp"
                        android:background="@drawable/counter_rectangle_shape"
                        android:orientation="vertical">
                        <ImageView
                            android:layout_width="match_parent"
                            android:background="@drawable/library"
                            android:layout_height="120dp" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Library"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:fontFamily="sans-serif-condensed"
                            android:textSize="16sp"
                            android:gravity="center"/>

                    </LinearLayout>


                </com.example.abc.itmcollegealigarh.view.MaterialRippleLayout>



            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="15dp"
                android:orientation="horizontal">

                <com.example.abc.itmcollegealigarh.view.MaterialRippleLayout
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:id="@+id/ll_digital_resource"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="2dp"
                        android:background="@drawable/counter_rectangle_shape"
                        android:orientation="vertical">
                        <ImageView
                            android:layout_width="match_parent"
                            android:background="@drawable/digitalresource"
                            android:layout_height="120dp" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Resources"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:fontFamily="sans-serif-condensed"
                            android:textSize="16sp"
                            android:gravity="center"/>

                    </LinearLayout>

                </com.example.abc.itmcollegealigarh.view.MaterialRippleLayout>


                <com.example.abc.itmcollegealigarh.view.MaterialRippleLayout
